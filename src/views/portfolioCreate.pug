extends layouts/main

block content
    .form-container
        form(action=routes.postCreate,method="post",enctype="multipart/form-data")
        input(type="text",name="title",placeholder="제목")
        textarea(name="description", cols="30", rows="10",placeholder="설명")
        input(type="file",name="portfolioImageFile")
        input(type="text",name="portfolioUrl",placeholder="포트폴리오 링크")
        input(type="submit",value="등록")
